{
  "compilerOptions": {
    "sourceMap": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "*": ["*"]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "src/**/*.png",
    "src/**/*.svg"
  ],
  "exclude": ["node_modules"]
}
